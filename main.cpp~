#include <stdlib.h>
#include <curses.h>
#include "objects.hpp"

#define APP "Starlit Colors"

using namespace std;

void quit() {
  endwin();
}

/**
 * Project: starlit colors
 * Creator: phill
 * Creation Date: Mi 19. Dez 14:26:25 CET 2018
 */
int main(int argc, char* argv[]) {
  initscr();
  atexit(quit);
  curs_set(0);

  mvprintw(3, 5, "Neues Project: %s", APP);

  getch();

  return 0;
}
